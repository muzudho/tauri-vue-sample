{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "vue-tauri-desktopapp-sample",
    "version": "0.1.0",
    "identifier": "com.vue-tauri-desktopapp-sample.app",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "vue-tauri-desktopapp-sample",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": {
            "../public/*" : "resources/",
            "../public_exe/*" : ""
        }
    }
}
